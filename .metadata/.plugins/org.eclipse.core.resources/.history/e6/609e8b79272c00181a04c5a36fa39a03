void calculateShelvingCoeff(float c_alpha, Int16* output)
{
	/* Your code here */
	Int32 vrednost = c_alpha*32768;
	if(vrednost>=32768){
		output=vrednost;
	}else if (vrednost<=-32768){
		output=-32768;
	}
}

