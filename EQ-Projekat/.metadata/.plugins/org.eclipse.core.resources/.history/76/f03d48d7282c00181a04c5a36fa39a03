void calculateShelvingCoeff(float c_alpha, Int16* output)
{
	/* Your code here */
		Int32 a =(Int16)c_alpha*32768;

		output[0] = a;
		output[1] = -32768;
		output[2] = 32767 - 1;
		output[3] = -a;


}

